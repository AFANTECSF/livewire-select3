{
  "name": "afantecsf/livewire-select3",
  "description": "A Select2-like dropdown component for Laravel Livewire",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Agustin Martin",
      "email": "agu.m.gg@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "livewire/livewire": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "afantecsf\\LivewireSelect3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "afantecsf\\LivewireSelect3\\Tests\\": "tests/",
      "afantecsf\\LivewireSelect3\\Tests\\Database\\Factories\\": "tests/database/factories/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "afantecsf\\LivewireSelect3\\LivewireSelect3ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test:coverage": "vendor/bin/pest --coverage"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}