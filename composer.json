{
  "name": "jekhar/livewire-select2",
  "description": "A Select2-like dropdown component for Laravel Livewire",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Agustin Martin",
      "email": "agu.m.gg@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "livewire/livewire": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "JekHar\\LivewireSelect2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JekHar\\LivewireSelect2\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JekHar\\LivewireSelect2\\LivewireSelect2ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}